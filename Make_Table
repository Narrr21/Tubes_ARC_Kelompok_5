CREATE DATABASE lumbung;
USE lumbung;

CREATE TABLE kulinerNangor (
	id INTEGER PRIMARY KEY,
    foodName VARCHAR(255) NOT NULL,
    foodOrDrink VARCHAR(255) NOT NULL,
    images VARCHAR(255) NOT NULL, 
    rating FLOAT NOT NULL
);

INSERT INTO kulinerNangor (id, foodName, foodOrDrink, images, rating)
VALUES
('1', 'Gado gado', 'Makanan', 'https://tse3.mm.bing.net/th?id=OIP.P9qQ2ewGJ-ugtUeeOZeCVQHaHa&pid=Api&P=0&h=220', '9.2')


SELECT * FROM kulinerNangor;

DROP TABLE kulinerNangor; # Run ini kalau mau Delete Table kulinernangor

## Tabel User

CREATE TABLE dataUser (
	uid INTEGER PRIMARY KEY AUTO_INCREMENT,
    userName VARCHAR(255) NOT NULL,
    profilePicture VARCHAR(255) NOT NULL,
    gender VARCHAR(1) NOT NULL, 
    userAge INTEGER NOT NULL,
    countFood INTEGER NOT NULL
);

INSERT INTO dataUser (userName, profilePicture, gender, userAge, countFood)
VALUES 
('John', 'john.jpg', 'L', 30, 0);

SELECT * FROM dataUser;

DROP TABLE dataUser;