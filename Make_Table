CREATE DATABASE lumbung;
USE lumbung;

CREATE TABLE kulinerNangor (
	id VARCHAR(20) PRIMARY KEY,
    foodName VARCHAR(255) NOT NULL,
    foodOrDrink VARCHAR(255) NOT NULL,
    images VARCHAR(255) NOT NULL, 
    rating VARCHAR(225) NOT NULL
);

